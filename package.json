{
  "name": "sonos-web-template-nextjs-typescript",
  "version": "1.4.0",
  "description": "Template for building NextJS applications using TypeScript",
  "main": "index.js",
  "repository": "https://github.com/Sonos-Inc/sonos-web-template-nextjs-typescript.git",
  "author": "andrewgadziksonos <andrew.gadzik@sonos.com>",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development next",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --quiet",
    "build": "yarn build:ssr",
    "build:ssg": "cross-env NODE_ENV=production next build && next export -o build",
    "build:ssr": "cross-env NODE_ENV=production next build",
    "start": "cross-env NODE_ENV=production next start",
    "release": "semantic-release",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/core": "10.1.1",
    "@preact/compat": "0.0.4",
    "@sonos-inc/web-ui-core": "1.9.3",
    "next": "10.2.3",
    "next-plugin-preact": "3.0.6",
    "preact": "10.5.13",
    "preact-render-to-string": "5.1.19",
    "preact-ssr-prepass": "1.1.3",
    "react": "npm:@preact/compat@0.0.4",
    "react-dom": "npm:@preact/compat@0.0.4",
    "react-ssr-prepass": "npm:preact-ssr-prepass@1.1.3"
  },
  "devDependencies": {
    "@emotion/babel-preset-css-prop": "^10.0.0",
    "@sonos-inc/cdk-core": "2.12.0",
    "@sonos-inc/cdk-nextjs": "2.12.0",
    "@sonos-inc/commitlint": "0.5.0",
    "@sonos-inc/eslint-config": "0.5.0",
    "@sonos-inc/jest": "0.5.0",
    "@sonos-inc/semantic-release": "0.5.0",
    "@sonos-inc/tsconfig": "0.5.0",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/styled-system": "^5.1.11",
    "constructs": "^3.3.75",
    "cross-env": "^7.0.3",
    "husky": "^6.0.0",
    "monocdk": "^1.106.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
